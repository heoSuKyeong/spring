 -->
 
 	<!-- 자식태그 -->
 <!-- 
 	<select id=""></select>
 	<insert id=""></insert>
 	<update id=""></update>
 	<delete id=""></delete>
  -->
 	<select id="time" resultType="String">
 		select sysdate from dual
 	</select>
 
 
</mapper>